    "@simplewebauthn/server": "^13.1.1",
    "@tailwindcss/vite": "^4.1.2",
    "tailwindcss": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "prisma": "^6.4.1",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "wrangler": "^4.4.0"
